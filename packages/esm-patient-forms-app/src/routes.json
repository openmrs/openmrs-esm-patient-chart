      "slot": "offline-tools-page-slot",
      "offline": true,
      "online": true,
      "meta": {
        "name": "forms",
        "slot": "offline-tools-page-forms-slot"
      }
    },
    {
      "name": "offline-tools-page-forms",
      "component": "offlineForms",
      "slot": "offline-tools-page-forms-slot",
