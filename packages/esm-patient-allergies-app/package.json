  },
  "dependencies": {
    "@carbon/react": "^1.3.0",
    "@openmrs/esm-patient-common-lib": "^3.2.0",
    "d3": "^7.0.3",
    "lodash-es": "^4.17.15"
  },
  "peerDependencies": {
    "@openmrs/esm-framework": "3.x",
    "@openmrs/esm-patient-common-lib": "3.x",
    "dayjs": "1.x",
