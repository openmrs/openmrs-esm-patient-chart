{
  "name": "@openmrs/esm-patient-chart",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lerna run lint && lerna run test && lerna run typescript"
    }
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "openmrs develop --sources 'packages/esm-*-app/'",
    "ci:publish": "lerna publish from-package --yes",
    "ci:prepublish": "lerna publish from-package --no-git-reset --yes --dist-tag next",
    "ci:release": "lerna version --no-git-tag-version",
    "ci:prerelease": "lerna version prerelease --no-git-tag-version --yes",
    "verify": "lerna run lint && lerna run test && lerna run typescript",
    "prettier": "prettier \"packages/**/src/**/*\" --write",
    "test": "jest --config jest.config.json --verbose false --passWithNoTests",
    "test-watch": "jest --watch --config jest.config.json",
    "coverage": "yarn test --coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.12.13",
    "@openmrs/esm-framework": "next",
    "@testing-library/dom": "^7.20.0",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "@testing-library/user-event": "^12.0.11",
    "@types/carbon-components-react": "^7.10.9",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.12",
    "@types/react-router-dom": "^5.1.7",
    "@types/lodash-es": "^4.17.3",
    "@types/webpack-env": "^1.16.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "browserslist-config-openmrs": "^1.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "css-loader": "^3.6.0",
    "dayjs": "^1.8.36",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-ts-react-important-stuff": "^3.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "husky": "^3.1.0",
    "i18next": "^19.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-cli": "^26.4.2",
    "lerna": "^3.20.2",
    "openmrs": "next",
    "prettier": "^2.2.1",
    "pretty-quick": "^2.0.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-i18next": "^11.0.0",
    "react-router-dom": "^5.0.0",
    "rxjs": "^6.6.3",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^1.2.1",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
