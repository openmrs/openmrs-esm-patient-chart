    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@openmrs/esm-error-handling": "^1.1.1",
    "@openmrs/react-root-decorator": "^3.2.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^9.5.0",
    "@types/jest": "^25.2.3",
    "@types/lodash-es": "^4.17.3",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/single-spa-react": "^2.12.0",
    "@types/systemjs": "^6.1.0",
    "@types/webpack-env": "^1.15.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "browserslist-config-openmrs": "^1.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "css-loader": "^3.6.0",
    "dayjs": "^1.8.36",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-ts-react-important-stuff": "^1.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-cli": "^26.4.2",
    "lodash-es": "^4.17.15",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "single-spa-react": "^2.14.0",
    "style-loader": "^1.2.1",
    "systemjs-webpack-interop": "^1.2.1",
    "typescript": "^3.9.7",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@openmrs/esm-api": "^3.0.0",
    "@openmrs/esm-extension-manager": "^0.1.1",
    "@openmrs/esm-module-config": "^2.2.0",
    "@openmrs/esm-patient-chart-widgets": "^1.0.3",
    "i18next": "^19.7.0",
