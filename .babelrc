{
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      ["i18next-extract", {
        "outputPath": "translations/{{locale}}.json",
        "discardOldKeys": true
      }]
    ]
  }